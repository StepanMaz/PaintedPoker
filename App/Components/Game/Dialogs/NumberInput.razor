<MudDialog>
    <DialogContent>
        <MudNumericField AutoFocus @bind-Value="number" />
    </DialogContent>
    <DialogActions>
        <MudButton OnClick="Submit" Color="Color.Success" Variant="Variant.Filled">Submit</MudButton>
    </DialogActions>
</MudDialog>

@code {
    [CascadingParameter]
    private MudDialogInstance MudDialog { get; set; } = null!;

    private int number;

    private void Submit() => MudDialog.Close(DialogResult.Ok(number));
}